      {"source": "header.timestamp", "alias": "created_at", "type": "timestamp"},
      {"source": "header.requestId", "alias": "request_id", "type": "string"},
      {"source": "body.orderId", "alias": "order_id", "type": "string"},
      {"source": "body.customerId", "alias": "customer_id", "type": "string"},
      {"source": "body.items[].sku", "alias": "sku", "type": "string"},
      {"source": "body.items[].name", "alias": "item_name", "type": "string"},
      {"source": "body.items[].quantity", "alias": "quantity", "type": "integer"},
      {"source": "body.items[].price", "alias": "price", "type": "float"}
    ]
  },
  "MeterValues": {
    "table_name": "meter_readings",
    "fields": [
      {"source": "header.action", "alias": "action", "type": "string"},
      {"source": "header.stationId", "alias": "station_id", "type": "string"},
      {"source": "header.connectorId", "alias": "connector_id", "type": "integer"},
      {"source": "body.transactionId", "alias": "transaction_id", "type": "integer"},
      {"source": "body.meterValue[].timestamp", "alias": "timestamp", "type": "timestamp"},
      {"source": "body.meterValue[].sampledValue[].measurand", "alias": "measurand", "type": "string"},
      {"source": "body.meterValue[].sampledValue[].value", "alias": "value", "type": "string"},
      {"source": "body.meterValue[].sampledValue[].unit", "alias": "unit", "type": "string"},
      {"source": "body.meterValue[].sampledValue[].context", "alias": "context", "type": "string"}
