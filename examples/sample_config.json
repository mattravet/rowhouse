  },
  "UserActivity": {
    "table_name": "user_events",
    "fields": [
      {"source": "header.action", "alias": "action", "type": "string"},
      {"source": "header.userId", "alias": "user_id", "type": "string"},
      {"source": "header.sessionId", "alias": "session_id", "type": "string"},
      {"source": "metadata.source.system.name", "alias": "source_system", "type": "string"},
      {"source": "metadata.source.system.version", "alias": "source_version", "type": "string"},
      {"source": "data.events[].eventType", "alias": "event_type", "type": "string"},
